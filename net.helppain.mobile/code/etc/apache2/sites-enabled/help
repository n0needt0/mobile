<VirtualHost  *:80>
  ServerName promis.helppain.net
  ServerAdmin webmaster@helppain.net
  AllowEncodedSlashes On
  DirectoryIndex index.php index.html
    SetEnv HTTPS 1
  RewriteEngine On

  DocumentRoot /var/www/help/web_roots/help
  Alias /assets/  /var/www/help/web_roots/help/assets/
  <Directory "/var/www/help/web_roots/help">
      Options All
      AllowOverride All
      Order allow,deny
      Allow from all
    RewriteEngine on
  </Directory>
</VirtualHost>
